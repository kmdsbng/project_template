{
    "compilerOptions": {
        "outDir": "./tscbuild/",
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "lib": ["es6", "dom"]
    },
    "include": [
        "./src/**/*"
    ]
}
