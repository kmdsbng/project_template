{
  "private": true,
  "name": "almin_typescript_template",
  "version": "1.0.0",
  "description": "Almin Typescript template",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf public/build && mkdirp public/build",
    "start": "webpack-dev-server --hot --content-base public/ --open",
    "build": "NODE_ENV=production webpack -p",
    "watch": "NODE_ENV=development webpack -d -w",
    "ava:tsc": "tsc --project tsconfig.ava.json",
    "ava:run": "cross-env NODE_PATH=testbuild/src ava testbuild/**/*.test.js",
    "ava:cover": "cross-env NODE_PATH=testbuild/src nyc ava 'testbuild/**/*.test.js'",
    "test": "run-s ava:tsc ava:cover"
  },
  "author": "azu",
  "license": "MIT",
  "dependencies": {
    "@types/classnames": "^2.2.0",
    "@types/node": "^8.0.13",
    "@types/power-assert": "^1.4.29",
    "@types/react": "^15.0.35",
    "@types/react-dom": "^15.5.1",
    "@types/uuid": "^3.4.0",
    "almin": "^0.12.1",
    "classnames": "^2.2.3",
    "power-assert": "^1.4.2",
    "prop-types": "^15.5.4",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "typescript.events": "^6.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/mocha": "^2.2.41",
    "ava": "^0.22.0",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.1.0",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.3",
    "ts-node": "^3.2.0",
    "typescript": "^2.5.2",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0"
  }
}
